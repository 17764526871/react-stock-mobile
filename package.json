{
  "name": "intellistock-mobile",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npm run lint:fix && npm run lint:js && npm run lint:style",
    "lint:fix": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:js": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx ./src",
    "lint:style": "prettier --write \"src/**/*.less\" && stylelint --fix \"src/**/*.less\"",
    "prepare": "husky install",
    "commitlint": "commitlint --config commitlint.config.cjs -e -V"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@react-vant/icons": "^0.0.10",
    "antd": "^5.12.6",
    "axios": "^1.6.3",
    "classnames": "^2.5.1",
    "cron": "^3.1.6",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "helux": "^4.2.5",
    "highlight.js": "^11.9.0",
    "lodash": "^4.17.21",
    "marked": "^11.1.1",
    "marked-highlight": "^2.1.0",
    "mockjs": "^1.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-router-dom": "^6.21.1",
    "react-vant": "^3.3.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.0",
    "less": "^4.2.0",
    "prettier": "^2.8.8",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.6.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-config-standard-less": "^3.0.1",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^1.2.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-eslint": "^1.8.1"
